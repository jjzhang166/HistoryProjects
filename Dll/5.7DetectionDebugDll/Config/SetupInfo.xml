<?xml version="1.0" encoding="utf-8"?>
<CommonTestSetup>
  <!--电压配置-->
  <Voltage SysVol="9">
    <node name="3v3电压">
      <!--单位:V-->
      <上限>3.55</上限>
      <下限>3.25</下限>
      <继电器IO>3</继电器IO>
    </node>
    <node name="1v8电压">
      <上限>1.92</上限>
      <下限>1.75</下限>
      <继电器IO>1</继电器IO>
    </node>
  </Voltage>
  <!--LED配置-->
  <LED>
    <!--单位:Ω-->
    <node name="LED电压">
      <高电平上限>18</高电平上限>
      <高电平下限>12</高电平下限>
      <低电平上限>18</低电平上限>
      <低电平下限>12</低电平下限>
      <继电器IO>3</继电器IO>
    </node>
  </LED>
  <!--电流配置-->
  <Current>
    <node name="12v工作电流">
      <!--单位:A-->
      <上限>0.899</上限>
      <下限>0.655</下限>
      <电压>12</电压>
    </node>
    <node name="16v工作电流">
      <!--单位:A-->
      <上限>0.699</上限>
      <下限>0.455</下限>
      <电压>16</电压>
    </node>
  </Current>
  <!--静态电流配置-->
  <StaticCurr>
    <node name="静态电流">
      <!--单位:uA-->
      <上限>50</上限>
      <下限>10</下限>
      <电压>12</电压>
      <IG>1</IG>
      <GND>2</GND>
      <继电器IO>3</继电器IO>
      <串口号>5</串口号>
    </node>
  </StaticCurr>
  <!--电阻配置-->
  <RES>
    <!--单位:Ω-->
    <node name="12V对地电阻">
      <上限>8000</上限>
      <下限>5000</下限>
      <继电器IO>0</继电器IO>
    </node>
  </RES>
</CommonTestSetup>
